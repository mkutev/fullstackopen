const App = () => {
	return (
		<div>
			<h1>Greetings</h1>
			<Hello name="Maya" age="26" />
			<br />
      	<Hello name="Nikolai" age="16" />
		</div>
	);
};

const Hello = ({name, age}) => {
	const bornYear = () => new Date().getFullYear() - age	
	return (
		<div>
			<p>Hello {name}, you are {age} years old.</p>
			<p>So you are probably born in {bornYear()}</p>
		</div>
	)
}

export default App;
